<html>
<head>
    <title>To-Do List</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

    <!--Styles-->
    <link href="/assets/styles.css" rel="stylesheet" type="text/css"/>

</head>
<body>
<section>
    <h1>My Todo List</h1>
    <div id="cars_table">
        <form>
            <input type="text" name="item" placeholder="Add New Item Here ..." required/>
            <button type="submit">
                Add Item
            </button>
        </form>
        <ul>
            <% for (var i = 0; i < cars.length; i++){ %>
                <li><%= cars[i].car %></li>
            <% } %>
        </ul>
    </div>
</section>
<script type="text/javascript" src="/assets/ajax-data.js"></script>
</body>
</html>